floor(n/2)
Rl <- function(n,sigma){
r1 <- runif(floor(n/2))
r2 <- 1-r1
r <- c(r1,r2)
rd <- numeric(length(r))
rd <- (-(2*sigma^2)*(log(1-r)))^(1/2)
return(rd)
}
