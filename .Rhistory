#Additional choice, use zip::zip to pack the two files into a single .zip file.
if(require("zip")){
d = read.table('input.txt',sep='\t',quote = '')
# Input 1: the homework assignment date
date = as.character(d[1,1]) #'2019-09-20'
# Input 2: the ID of the reviewer (you)
reviewer = d[1,2] # '19003'
# Input 3: scores for the three submissions
name=paste("R",reviewer,date,sep="-");
zip::zip(paste0(name,".zip"),c('Scoring.html','Scoring.Rmd','score.csv'))
cat(paste0("To finish review process, submit the .zip file to the TA.\n"))
}else{
cat("You need to install R package 'zip'.\n")
}
